/* 整体*/

@charset "utf-8";
*{
	margin:0px;
	padding:0px;}
	
body {
    font-family:arial,"Hiragino Sans GB","Microsoft Yahei",sans-serif;;
    color: rgb(82, 82, 82);
	background-color: rgb(240, 240, 240);
	font-size:14px;
	line-height:25px;
	
}

p{
	margin-bottom:8px;
	margin-top:8px;
}

#web{
	width:100%;
	margin-left:auto;
	margin-right:auto;}
	
a{
	display:block;
	color:inherit;
	text-decoration:none;
	
	}

img{
	border:0px;/*解决IE中自动给带链接的img加边框的问题*/
}
	



/*头部*/
#bghead{
	width:100%;
	min-width:1200px;/*解决将浏览器半屏显示时，拖动水平滚动条到右边时，导航条和页尾没有背景颜色的问题*/
	height:50px;
	float:left;
	background:rgb(34,34,34);
}

#header{
	width:1000px;
	height:50px;
	margin:0px auto;
	font-family: '΢���ź�','Helvetica Neue',Helvetica,Arial,sans-serif !important;
	font-size:14px;
	position:relative;
	}

/*头部导航条*/	

#logo{
	width:235px;
	height:50px;

	float:left;}


#logo a img{/*控制图片自动缩放*/

    max-width:235px;
	max-height:50px;

}

 


#nav{
	height:50px;
	position:relative;
	margin-left:10px;
	float:left;
	width:480px;}

	
#nav ul{
	display:block;
	position:relative;}
	
#nav ul li{
	display:block;
	list-style-type:none;
	height:50px;
	float:left;
	}
	
#nav ul li a{
	display:block;
	color:#FFFFFF;
	text-decoration:none;
	padding:0px 12px;
	height:50px;
	line-height:50px;
	}
	
#nav ul li a:hover{
	color:rgb(255,255,255);
	background:rgb(51, 157, 210);}
	
#nav ul li a:active{
	background:rgb(13,164,211);}


	
#nav li ul{
	display:none;
	width:100%;
	left:0px;
	background:rgb(34,34,34);
	text-align:center;
	position:absolute;
	z-index:500;
}

#nav li:hover ul{
	display:block;
}

#nav ul li{
	list-style-type:none;
	position:relative;}
	
#nav ul li ul li{
	border-top:1px solid #666666;
	width:100%;
	height:50px;
}

#nav ul li ul li a{
	display:block;
	height:50px;
	line-height:50px;
	font-size:12px;
	}
	
#article_add{
	height:50px;
	width:95px;
	left:30px;
	position:relative;
	float:left;
}

#article_add ul{
	list-style-type:none;
}

#article_add ul li a{
	display:block;
	color:#FFFFFF;
	text-align:center;
	text-decoration:none;
	height:50px;
	font-size:14px;
	line-height:50px;
	color:rgb(255,255,255);
	background:rgb(51, 157, 210);
}

#article_add ul li a:hover{
	color:rgb(255,255,255);
	background:rgb(51, 157, 210);}
	
#article_add ul li a:active{
	background:rgb(13,164,211);}

#login{
	left:30px;
	width:170px;
	height:50px;
	float:right;
	position:relative;
}


#login ul{
	float:left;
	display:block;
	list-style-type:none;
	position:relative;}
	
#login ul li{
	display:block;
	height:28px;
	float:left;
}
	
#login ul li a{
	display:block;
	color:#FFFFFF;
	text-align:center;
	min-width:36px;
	height:50px;
	line-height:50px;
	text-decoration:none;
	font-size:12px;
	float:left;
}


	
#login ul li a:hover{
	color:rgb(255,255,255);
	background:rgb(51, 157, 210);}
	
#login ul li a:active{
	background:rgb(13,164,211);}
	

/*中间部分*/	
#container{
	width:100%;
	padding-top:60px;
	float:left;  
	position:relative;}
	
#container-wrap{
	background: rgb(250, 250, 252);
	width:1000px;
	height:auto;
	padding:10px;
	margin:0px auto;
	box-shadow: 2px 0px 5px rgba(214, 214, 215, 0.9), -2px 0px 5px rgba(214, 214, 215, 0.9);/*添加边框阴影*/
	}

/*左侧*/	
#aside-left{
	width:19%;
	float:left;}
/*左侧导航栏*/	
#nav-aside{
	width:100%;
	background:rgb(34,34,34);
	position:relative;}
	
ul li{
	list-style-type:none;
	position:relative; }/*position:relative;这句很重要，添加后就可以使二级菜单的顶部与鼠标划过的链接平行*/
	
#nav-aside ul li a{
	display:block;
	color:rgb(204,204,204);
	text-decoration:none;
	padding-left:20px;
	padding-top:0px;
	padding-bottom:0px;
	height:40px;
	line-height:40px;
	font-family: '΢���ź�','Helvetica Neue',Helvetica,Arial,sans-serif !important;
	font-size:14px;
	border-bottom:1px solid;
	border-color:#666666;}
	
#nav-aside ul li a:hover{
	color:rgb(255,255,255);}
	
span.arrow{
	float:right;
	margin-right:10px;
	color:#CCCCCC;
	line-height:40px;
	margin-bottom:auto;
	margin-top:auto;}
	
#nav-aside li ul{
	display:none;
	left:100%;
	top:0px;
	width:100%;
	border-left:1px solid #666666;
	background:rgb(34,34,34);
	text-align:center;
	position:absolute;
	z-index:500;
}
#mynav li {height:150px;


}	
#mynav li.over ul{
	display:block;
	}
	
/*图书推荐*/
#books-recommended{
	margin-top:100px;
}

#books-recommended a{
	font-size:1em;
	line-height:1.4em;
	overflow:hidden;
	font-family: Arial,sans-serif;
	
}

.content-list li{
	border-bottom: 1px dotted rgb(204, 204, 204);
	padding:1em 0;
}



#books-recommended dt{
	color: rgb(50, 50, 50);
	
}

#books-recommended dd{
	font-size:0.95em;
	line-height:1.1em;
	color: rgb(113, 113, 113);
}


.content-title{
	color: rgb(153, 153, 153);
	font-family: '微软雅黑',hei,'黑体',Arial,Helvetica,sans-serif;
	font-size:1em;
	padding-left:1em;
	background: rgb(243, 243, 243);
	border-left: 10px solid rgb(233, 233, 233);

}

.book-img{
	width:60px;
	float:left;
	margin-right:1em;
}





/*主体内容*/	
#main{
	width:80%;
	float:left;
	position:relative;}
	
#content{
	width:65%;
	padding:0px 15px;
	float:left;}
	
#content .content-title{
	line-height:2em;
	font-size:1.2em;
}

#content-essay{
	width:100%;
	padding:0px 20px;
	float:left;
}
	
	
.essay{
	border-bottom: 1px dotted rgb(204, 204, 204);
	padding:1em;
	overflow:hidden;
}

.essay-title a{
	color: rgb(13, 164, 211);
	text-decoration:none;
	line-height:45px;}
	
.essay-metas{
	color: rgb(187, 187, 187);}
	
.essay-summary{
	margin:15px 0px;}


/*分页导航条*/
#pages-nav{
	width:100%;
	float:left;
}

#pages-nav a, #pages-nav .current{
	width:15px;
	float:left;
}





/*右侧*/	
#aside-right{
	width:30%;
	float:right;}

#aside-right li{
	height: 28px;
	font-size: 13px;
	overflow: hidden;
}

#aside-right b{
	display:block;
	font-size:10px;
	margin-top:6px;
	margin-right:5px;
	text-align:center;
	background:rgb(200, 199, 199);
	height:14px;
	line-height:14px;
	width:14px;
	color: rgb(255, 255, 255);
	font-weight:100;
	float:left;
}

#aside-right a{
	display:block;
	color:#0066FF;
	text-decoration:none;
	font-family: '΢���ź�','Helvetica Neue',Helvetica,Arial,sans-serif !important;
	font-size:1em;
}

#aside-right a:hover{
	color:#000000;
}

#aside-right .num-1 b, #aside-right .num-2 b, #aside-right .num-3 b{
	background: rgb(254, 72, 0);
}

/*页尾*/

.footer{
	width:100%;
	min-width:1200px;/*解决将浏览器半屏显示时，拖动水平滚动条到右边时，导航条和页尾没有背景颜色的问题*/
	height:300px;
	background:rgb(170, 170, 170);
	float:left;
	color:rgb(255, 255, 255);
}

#footer-bg{
	width:100%;
	min-width:1200px;/*解决将浏览器半屏显示时，拖动水平滚动条到右边时，导航条和页尾没有背景颜色的问题*/
	float:left;
	background:url(../images/footer-bg.png);
}

#footer-inner{
	width:940px;
	margin: 0px auto;
	
}

.inner-block{
	width:25%;
	height:300px;
	float:left;

}

.footer-content{
	color: rgb(138, 138, 138);
	font-size:12px;
	padding-right:1em;
}



.footer-content a.pic-link-small{
	display:block;
	width:66px;
	height:66px;
	border:2px solid;
	float:left;
	opacity:0.6;
}
.footer-content a.pic-link-small:hover{
  opacity:1;
}

.pic-link-small img{ 
	width:66px; 
	height:66px;
}

a.pic-link-big{
	display:block;
	width:200px;
	height:200px;
}
.col{ 
	float:left; 
	margin-left:20px;

}
.col a{ 
	color:#999999; 
	font-size:11px;
 

}
.col a:hover{ 
    color:#FFFFFF;

}
a.right{
	display:block;
	text-align:center;
	margin-top:1em;
}

a.white:hover{
	color:#FFFFFF;
}

.footer-bottom{
	width:100%;
	height:111px;
	background:url(../images/footer_bottom_bg.png);
	float:left;
}

p.copyright{
	display:block;
	width:30%;
	margin-left:20%;
	margin-top:1em;
	text-align:center;
	color: rgb(138, 138, 138);
}


/*回到顶部*/
#back2top{
	width:51px;
	height:51px;
	position:fixed;
	right:100px;
	bottom:30px;
	display:none;
}

#back2top a{
	display:block;
	height:100%;
	width:100%;
	background:url(../images/back2top.gif) top;
}

#back2top a:hover{
	background:url(../images/back2top.gif) bottom;
}


/*因为浮动元素不占据空间，所以容器元素不包围他们。添加一个进行清理的空元素可以迫使容器元素包围浮动元素*/
.clear{
	clear:both;
}


